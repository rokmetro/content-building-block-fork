                "description": "Retrieves  all items",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "StudentGuides"
                ],
                "operationId": "getAllStudentGuides",
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "AdminUserAuth": []
                    }
                ],
                "description": "Retrieves  all items",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "StudentGuides"
                ],
                "operationId": "CreateStudentGuide",
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        },
        "/student_guides/{id}": {
            "get": {
                "security": [
                    {
                        "RokwireAuth AdminUserAuth": []
